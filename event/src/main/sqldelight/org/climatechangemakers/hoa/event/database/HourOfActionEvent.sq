CREATE TABLE hour_of_action_event(
  id TEXT NOT NULL PRIMARY KEY,
  event_start TIMESTAMPTZ NOT NULL,
  event_name TEXT NOT NULL
);

insertEvent:
INSERT INTO hour_of_action_event(id, event_start, event_name)
VALUES(:id, DATE_TRUNC('minute', CAST(:eventStart AS TIMESTAMPTZ)), :eventName)
ON CONFLICT(id) DO UPDATE
SET id = :id, event_start = DATE_TRUNC('minute', CAST(:eventStart AS TIMESTAMPTZ)), event_name = :eventName;